<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox fx:id="vBox" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefWidth="500.0" spacing="3.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cookbook.Controller.WeeklyListItemController">
   <children>
      <HBox alignment="CENTER" onMouseClicked="#openWeeklyList" prefHeight="0.0" prefWidth="0.0" style="-fx-border-width: 1; -fx-border-color: #bbb; -fx-border-radius: 7;">
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <children>
            <Text fx:id="weekName" strokeType="OUTSIDE" strokeWidth="0.0" text="My Week" />
            <Region prefHeight="0.0" prefWidth="200.0" />
            <Text fx:id="dateOfWeek" strokeType="OUTSIDE" strokeWidth="0.0" text="06/05/2024 - 13/05/2024" />
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
   </padding>
</VBox>
